  "scripts": {
    "start": "nx serve --configuration=development",
    "build": "nx build",
    "debug": "nx serve --configuration=development",
    "test": "nx test",
    "lint": "nx lint",
    "e2e": "nx e2e",
    "webpack": "nx webpack",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "changed:packages": "changeset status --output=changed-packages.json",
    "release": "pnpm changeset tag",
    "db:migrate": "nx run-many --target=migrate --projects=tag:knex --verbose && nx run-many --target=prisma-deploy --projects=tag:prisma --verbose",
    "db:up": "docker-compose -f resources/docker-compose.yml up -d",
    "db:down": "docker-compose -f resources/docker-compose.yml down",
    "create:library": "nx g @nx/js:library --name=$LIBRARY_NAME --unitTestRunner=none --bundler=none --directory=libs --importPath=@frnt/$LIBRARY_NAME --minimal && nx g @nx/react:storybook-configuration --name=$LIBRARY_NAME --no-configureCypress --no-configureStaticServe --no-generateCypressSpecs --no-generateStories",
    "create:component": "nx g @nx/react:component --name=$COMPONENT_NAME --project=$LIBRARY_NAME --directory=/lib/$DIRECTORY_NAME --export --inSourceTests && nx g @nx/react:component-story --componentPath=/lib/$DIRECTORY_NAME/$COMPONENT_NAME/$COMPONENT_NAME.js --project=$LIBRARY_NAME",
    "rm:node_modules": "npx rimraf --glob **/node_modules",
    "base-ui:storybook": "nx serve --watch base-ui:storybook",
    "account-ui:storybook": "nx serve --watch account-ui:storybook",
    "market-data-ui:storybook": "nx serve --watch market-data-ui:storybook",
    "trade-ui:storybook": "nx serve --watch trade-ui:storybook"
  },
